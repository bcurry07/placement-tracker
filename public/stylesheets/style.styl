body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF
select.input-sm
  color: #272b30

.badge
  margin-left: 5px

.cursor:hover
  cursor:pointer

.cancel_button
  margin-left: 10px

.graph_year
  font-style: italic
  font-size: 72px

.addIcon
  margin-left: 5px
  margin-right: 5px

.deleteIcon
  display: none

tr:hover .placementName, tr:hover .clientName
  text-decoration: underline
  font-weight: bold

tr:hover .deleteIcon
  display: block

.deleteCell
  width: 40px

.strong
  font-weight: bold

span.glyphicon-filter
  font-size: 0.75em;
  vertical-align: text-top

.filterButton
  height: 25px

.No
  color: #ff0000

.Yes
  color: green

.placementRow
  height: 30px

.placementNameColumn
  width: 220px

.clientNameColumn
  width: 120px

.billingIcon
  margin-left: 25px

tr.placementRow:hover td
  text-decoration: underline

.clientInput
  margin-bottom: 40px

.formMenu
  margin-left: 15px


